modules = ["nodejs-20"]

[nix]
channel = "stable-25_05"

[workflows]

[[workflows.workflow]]
name = "Run"
mode = "sequential"
author = 45909031

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 24678
externalPort = 80
exposeLocalhost = true

[deployment]
run = ["sh", "-c", "npm run dev"]
deploymentTarget = "cloudrun"
